#!/usr/bin/env php
<?php

$autoloadFiles = [__DIR__ . '/../vendor/autoload.php', __DIR__ . '/../../../vendor/autoload.php', __DIR__ . '/../../../autoload.php'];
foreach ($autoloadFiles as $autoloadFile) {
    if (file_exists($autoloadFile)) {
        require_once $autoloadFile;
        break;
    }
}

use SymfonyDocsBuilder\Application;
use SymfonyDocsBuilder\DocsKernel;

DocsKernel::create()->get(Application::class)->run();
